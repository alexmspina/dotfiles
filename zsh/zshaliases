# Safari Alias
alias safari="open -a safari"

# Coding Time
alias coding-time="safari && code-insiders ."

# replace rm with trash
alias rm="trash $1"

# tmuxinator
alias tmx="tmuxinator"

# obsidian
alias obsidian="open -a obsidian"

# docker
alias docker="lima nerdctl"

# thefuck
eval $(thefuck --alias)
alias shoot="fuck"

# Open Goland from terminal
alias goland="open -na \"GoLand.app\" --args \"$@\""

# Replace goimports with gosimports
alias goimports="gosimports"

# remove all images
# alias rmimgs="docker rmi $(docker images -q)"

# remove all containers
# alias rmconts="docker rm $(docker ps -q)"
